{
  "name": "sholong",
  "description": "An app",
  "version": "0.1.0",
  "scripts": {
    "start": "vite",
    "dev": "($npm_execpath outdated || read -p \"Press enter to ignore...\") && vite --host",
    "build": "vite build && sed 's+media=\"(device+media=\"screen and (device+g' dist/index.html | sed 's+</head>+<meta name=\"apple-touch-fullscreen\" content=\"yes\" /></head>+g' > index.html.tmp && mv index.html.tmp dist/index.html",
    "prod": "$npm_execpath run build && vite preview --host",
    "serve": "vite preview"
  },
  "license": "MIT",
  "dependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@solid-primitives/event-listener": "^2.2.14",
    "@solid-primitives/intersection-observer": "^2.1.3",
    "@solid-primitives/media": "^2.2.4",
    "@solid-primitives/mouse": "^2.0.15",
    "@solid-primitives/resize-observer": "^2.0.20",
    "@solid-primitives/timer": "^1.3.7",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "fast-sort": "^3.4.0",
    "lightweight-charts": "^4.0.1",
    "marked": "^7.0.5",
    "solid-js": "^1.7.11"
  },
  "devDependencies": {
    "@darkobits/vite-plugin-favicons": "^0.3.0",
    "@iconify-json/tabler": "^1.1.89",
    "@tailwindcss/container-queries": "^0.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/marked": "^5.0.1",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-solid-svg": "^0.6.4"
  }
}
